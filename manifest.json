{
  "manifest_version": 2,
  "name": "Humans.txt Reader",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.0.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon19-default.png",
      "38": "images/icon38-default.png"
    }
  }
}
